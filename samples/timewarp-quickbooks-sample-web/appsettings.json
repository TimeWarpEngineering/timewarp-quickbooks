{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "QuickBooksOAuth": {
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret",
    "RedirectUri": "https://localhost:5024/callback",
    "Environment": "Sandbox",
    "Scopes": [
      "Accounting"
    ]
  },
  "QuickBooksApi": {
    "Environment": "Sandbox",
    "TimeoutSeconds": 30,
    "EnableDetailedLogging": true,
    "MinorVersion": "65"
  }
}
